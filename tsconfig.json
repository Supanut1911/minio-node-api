{
  "compilerOptions":  {
    "target": "es2017",                          
    "module": "commonjs",                    
    "lib": ["es6"],                     
    "allowJs": true,
    "outDir": "dist",                          
    "rootDir": "src",
    "strict": true,         
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "strictFunctionTypes": false,
    "skipLibCheck": true
  },"include": [
    "src/*", 
    "src/common/fn.ts" 
    ],"exclude": [
    "node_modules",
    "./node_modules",
    "./node_modules/*",
    "documentation",
  ]
}